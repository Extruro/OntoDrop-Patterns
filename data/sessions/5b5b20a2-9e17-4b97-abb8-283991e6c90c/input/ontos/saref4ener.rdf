    <rdfs:label xml:lang="en">Fill level target profile element</rdfs:label>
    <rdfs:comment>A description of the various Elements detailing the Fill Level Target Profile consists.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4ener/MaxDuration">
    <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</owl:deprecated>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:label xml:lang="en">Max duration</rdfs:label>
