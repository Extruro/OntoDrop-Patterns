    <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:label xml:lang="en">Elapsed slot time</rdfs:label>
    <rdfs:comment xml:lang="en">If state is set to 'running' or 'paused' AND the slot is determined, this element CAN contain the time the slot has already been in 'running' state (this also means the value remains constant during a 'paused' state). Otherwise it SHALL be omitted.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4ener/IncentiveTableTier">
