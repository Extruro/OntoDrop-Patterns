    <rdfs:domain rdf:resource="https://saref.etsi.org/saref4ener/PowerLimit"/>
    <rdfs:comment>Indicates whether the power limit is currently active</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4ener/deviceName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">device name</rdfs:label>
    <rdfs:comment xml:lang="en">Provides the name of the device as defined by the manufacturer.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4ener/hasMaxPauseDuration">
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4ener/hasDuration"/>
