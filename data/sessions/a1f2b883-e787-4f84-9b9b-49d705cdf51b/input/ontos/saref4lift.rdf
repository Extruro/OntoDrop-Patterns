    <dcterms:description xml:lang="en">This ontology extends the SAREF ontology for the Smart Lifts domain.</dcterms:description>
    <owl:imports rdf:resource="https://saref.etsi.org/saref4syst/v1.1.2/"/>
    <owl:imports rdf:resource="https://saref.etsi.org/core/v3.1.1/"/>
    <dcterms:creator rdf:resource="https://www.linkedin.com/in/mahdi-bennara/"/>
